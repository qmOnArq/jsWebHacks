  },
  "homepage": "https://github.com/qmOnArq/jsWebHacks#readme",
  "devDependencies": {
    "@types/file-saver": "^2.0.5",
    "@types/jquery": "^3.5.14",
    "@types/lodash": "^4.14.182",
    "clean-webpack-plugin": "^4.0.0",
    "jszip": "^3.10.0",
    "lodash": "^4.17.23",
    "prettier": "^2.7.1",
    "svelte": "^3.49.0",
    "svelte-loader": "^3.1.3",
    "svelte-preprocess": "^4.10.7",
    "terser-webpack-plugin": "^5.3.3",
